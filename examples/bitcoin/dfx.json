{
    "canisters": {
        "bitcoin": {
            "type": "azle",
            "main": "src/index.ts",
            "candid_gen": "automatic",
            "env": ["AZLE_TEST_FETCH"],
            "declarations": {
                "output": "test/dfx_generated/bitcoin",
                "node_compatibility": true
            }
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:8000",
            "type": "ephemeral",
            "replica": {
                "subnet_type": "system"
            }
        }
    },
    "defaults": {
        "bitcoin": {
            "enabled": true,
            "nodes": ["127.0.0.1:18444"],
            "log_level": "info"
        }
    }
}
