type Post = record {
  id : text;
  "text" : text;
  author : record {
    id : text;
    username : text;
    threads : vec record {
      id : text;
      title : text;
      author : User;
      posts : vec Post;
    };
    posts : vec Post;
    reactions : vec record {
      id : text;
      post : Post;
      author : User;
      reactionType : variant { thumbsDown; fire; thumbsUp };
    };
  };
  thread : record { id : text; title : text; author : User; posts : vec Post };
  reactions : vec record {
    id : text;
    post : Post;
    author : User;
    reactionType : variant { thumbsDown; fire; thumbsUp };
  };
};
type Reaction = record {
  id : text;
  post : Post;
  author : User;
  reactionType : ReactionType;
};
type ReactionType = variant { thumbsDown; fire; thumbsUp };
type Thread = record {
  id : text;
  title : text;
  author : User;
  posts : vec Post;
};
type User = record {
  id : text;
  username : text;
  threads : vec Thread;
  posts : vec Post;
  reactions : vec Reaction;
};
service : () -> {
  createPost : (text, text, text, nat32) -> (Post);
  createReaction : (text, text, ReactionType, nat32) -> (Reaction);
  createThread : (text, text, nat32) -> (Thread);
  createUser : (text, nat32) -> (User);
  getAllPosts : (nat32) -> (vec Post) query;
  getAllReactions : (nat32) -> (vec Reaction) query;
  getAllThreads : (nat32) -> (vec Thread) query;
  getAllUsers : (nat32) -> (vec User) query;
}