{
    "canisters": {
        "robust_imports": {
            "type": "custom",
            "build": "npx azle robust_imports",
            "root": "src",
            "ts": "src/index.ts",
            "candid": "src/index.did",
            "wasm": ".azle/robust_imports/robust_imports.wasm.gz",
            "declarations": {
                "output": "dfx_generated/robust_imports",
                "node_compatibility": true
            },
            "opt_level": "1"
        },
        "not_azle": {
            "type": "custom",
            "build": "npx azle not_azle",
            "root": "src",
            "ts": "src/not_azle/missing_type.ts",
            "candid": "src/missing_type.did",
            "wasm": ".azle/not_azle/not_azle.wasm.gz",
            "declarations": {
                "output": "dfx_generated/not_azle",
                "node_compatibility": true
            }
        }
    }
}
