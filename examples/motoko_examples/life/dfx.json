{
  "canisters": {
    "life": {
      "type": "custom",
      "build": "npx azle life",
      "root": "src/life",
      "ts": "src/life/main.ts",
      "candid": "src/declarations/life.did",
      "wasm": "target/wasm32-unknown-unknown/release/life.wasm",
      "declarations": {
        "output": "src/declarations"
      }
    },
    "life_assets": {
      "dependencies": [
        "life"
      ],
      "frontend": {
        "entrypoint": "src/life_assets/public/index.html"
      },
      "source": [
        "src/life_assets/assets",
        "dist/life_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.9.3",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
