{
    "canisters": {
        "hello": {
            "type": "azle",
            "main": "src/hello/index.ts",
            "candid_gen": "automatic",
            "declarations": {
                "output": "dfx_generated/hello",
                "node_compatibility": true
            }
        },
        "hello_assets": {
            "type": "assets",
            "dependencies": ["hello"],
            "frontend": {
                "entrypoint": "src/hello_assets/src/index.html"
            },
            "source": ["src/hello_assets/assets", "dist/hello_assets/"]
        }
    },
    "defaults": {
        "build": {
            "args": "",
            "packtool": ""
        }
    },
    "version": 1
}
